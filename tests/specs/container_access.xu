func assert(x) { __builtin_assert(x) }

let a = [1, 2, 3][0]
assert(a == 1)

let l = [1, 2, 3]
assert(l.contains(2))
assert(l.length() == 3)

let d = {"a": 1}
let got = d.get("a")
match got {
    Option#some(x) { assert(x == 1) }
    Option#none    { assert(false) }
    _ { assert(false) }
}
assert(d.length() == 1)
