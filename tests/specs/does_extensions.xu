func assert(x) { __builtin_assert(x) }

// does extension methods && multiple blocks
User has {
    name: string
    age: int
}

User does {
    func to_json() -> string {
        return """{"name": "{self.name}", "age": {self.age}}"""
    }
}

User does {
    func method1() -> string { return "m1" }
}

User does {
    func method2() -> string { return "m2" }
}

let u = User{ name: "Alice", age: 20 }
assert(u.to_json() == """{"name": "Alice", "age": 20}""")
assert(u.method1() == "m1")
assert(u.method2() == "m2")
