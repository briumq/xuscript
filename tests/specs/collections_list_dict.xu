func assert(x) { __builtin_assert(x); }

// list
let l: [int] = [];
l.add(1);
l.add(2);
l.add(3);
assert(l.length() == 3);
assert(l[0] == 1);
assert(l[1] == 2);
assert(l[2] == 3);
assert(l.contains(2));

// dict
let d: {string: int} = {};
d.insert("a", 1);
let got = d.get("a");
match got {
    Option#some(x) { assert(x == 1); }
    Option#none    { assert(false); }
    _ { assert(false); }
};
assert(d.length() == 1);
assert(!(d.contains("b")));
