func assert(x) { __builtin_assert(x) }

let dict = {"a": 1, "b": 2}
var sum_values = 0
var keys_str = ""

for k in dict {
    let v = dict[k]
    if k is "a" { 
        assert(v is 1)
        sum_values += v 
    }
    if k is "b" { 
        assert(v is 2)
        sum_values += v 
    }
    // Simple concat to check keys are visited
    // Note: order is undefined, so we don't assert keys_str exact value, just usage
    if keys_str is "" {
        keys_str = k
    } else {
        // keys_str = keys_str + k
    }
}

assert(sum_values is 3)
