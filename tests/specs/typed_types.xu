func assert(x, msg="Assertion failed") { __builtin_assert(x, msg) }

// typed_dict_empty
let d: {string: int} = {}
assert(d.length() == 0, "dict length")

// typed_set_empty (set is implemented as dict)
let s = set{}
assert(s.length() == 0, "set length")

// typed_tuple_return
func divmod(a: int, b: int) -> (int, int) {
    return (a / b, a % b)
}
let (q, r) = divmod(10, 3)
assert(q == 3, "q")
assert(r == 1, "r")

