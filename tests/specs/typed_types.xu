func assert(x, msg="Assertion failed") { __builtin_assert(x, msg) }

// typed_dict_empty
let d: {string: int} = {}
assert(d.length is 0, "dict length")

// typed_set_empty
let s: {int} = set{}
assert(s.length is 0, "set length")

// typed_tuple_return
func divmod(a: int, b: int) -> (int, int) {
    return (a / b, a % b)
}
let (q, r) = divmod(10, 3)
assert(q is 3, "q")
assert(r is 1, "r")

