// 字符串方法测试
func assert(x) { __builtin_assert(x) }

let s = "  Hello, World!  ";
assert(s.trim() == "Hello, World!");
assert(s.trim_start() == "Hello, World!  ");
assert(s.trim_end() == "  Hello, World!");

let s2 = "Hello, World!";
assert(s2.find("Hello").has());
assert(s2.substr(0, 5) == "Hello");

// replace 只替换第一个匹配
let s3 = "Hello, Hello";
assert(s3.replace("Hello", "Hi") == "Hi, Hello");

print("All string method tests passed!");