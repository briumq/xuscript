// 测试字典键值对循环实现

// 测试函数
func test_dict_key_value_loop_implementation() {
    println("=== 测试字典键值对循环实现 ===")

    // 创建一个字典
    let dict = {
        "name": "Alice",
        "age": 30,
        "city": "New York",
        "country": "USA"
    }

    // 测试1: 字典键值对循环
    println("\n测试1: 字典键值对循环")
    println("期望: 能够遍历字典的键值对")
    for (k, v) in dict {
        println("{k}: {v}")
    }

    // 测试2: 普通的字典键循环
    println("\n测试2: 普通的字典键循环")
    println("期望: 能够遍历字典的键")
    for k2 in dict {
        println("Key: {k2}")
    }

    // 测试3: 字典键值对循环计算总和
    println("\n测试3: 字典键值对循环计算总和")
    println("期望: 能够计算字典中数值的总和")
    let numbers = {
        "a": 10,
        "b": 20,
        "c": 30,
        "d": 40
    }
    var sum: int = 0
    for (k3, v3) in numbers {
        sum += v3
    }
    println("Sum: {sum}")
}

// 主函数
func main() {
    test_dict_key_value_loop_implementation()
}

// 调用主函数
main()