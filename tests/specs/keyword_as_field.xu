// Test keywords as field names
func assert(x, msg="Assertion failed") { __builtin_assert(x, msg) }

// Test with dict
let obj = { has: 1, is: 2, match: 3 }
assert(obj.has == 1, "dict.has")
assert(obj.is == 2, "dict.is")
assert(obj.match == 3, "dict.match")

// Test with struct
Test has {
    has: int
    is: string
}

let t = Test{ has: 42, is: "hello" }
assert(t.has == 42, "struct.has")
assert(t.is == "hello", "struct.is")
