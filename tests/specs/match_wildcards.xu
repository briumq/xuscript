// test: match_wildcard
func assert(x) { __builtin_assert(x) }

let t = (1, 2, 3)
let result = match t {
    (0, _, _) { "starts with zero" }
    (_, 0, _) { "middle is zero" }
    _ { "none zero" }
}
assert(result is "none zero")

let t2 = (0, 5, 5)
let r2 = match t2 {
    (0, _, _) { "starts with zero" }
    _ { "other" }
}
assert(r2 is "starts with zero")
