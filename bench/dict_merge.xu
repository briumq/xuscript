use "time" as time

size = 200
A = dict{}
B = dict{}
for i in 0..size {
    A["k{i}"] = i
    B["k{i}"] = i + 1
}

warm = 2000
for i in 0..warm {
    A.merge(B)
}

iters = 200000
t0 = time.mono_nanos()
for i in 0..iters {
    A.merge(B)
}
t1 = time.mono_nanos()

total = t1 - t0
per = total / iters

println("lang=xu iters={iters} total_ns={total} per_ns={per}")
