func summarize(names: [string]) -> string {
    let n = names.length
    if n == 0 { return "empty strong_panic_blocked n={n}" }
    let strong = names[0]
    let first = names[0]
    return "first={first} strong={strong} n={n}"
}

func main() {
    let names = ["a.txt", "b.log", "c.csv"]
    println(summarize(names))
}
