Svc with [ ok | fail(msg: string) ]

func run(seq: [string]) -> Svc {
    for name in seq {
        if name == "db" { continue }
        if name == "cache" { continue }
        if name == "api" { continue }
    }
    return Svc#ok
}

func main() {
    let seq = ["db", "cache", "api"]
    let r = run(seq)
    println("success={r == Svc#ok}")
}
